	<xsd:complexType name="EPMEmploymentDetailsType">
		<xsd:annotation>
			<xsd:documentation source="http://www.hr-xml.org" xml:lang="en">
				<ccts:DictionaryEntryName>Employee Performance Management_ Employment. Details</ccts:DictionaryEntryName>
				<ccts:DefinitionText>Contains a collection of data about a person's employment relevant to employee performance management. Appraisal subjects most always will be employees, but not all EPM participants are employees. </ccts:DefinitionText>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="IndicativeEmploymentDetailsType"/>
		</xsd:complexContent>
