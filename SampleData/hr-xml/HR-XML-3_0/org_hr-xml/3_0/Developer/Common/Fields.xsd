			<xsd:extension base="SpecimenCollectionCodeContentType">
				<xsd:attributeGroup ref="CodeListAttributeGroup"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:element name="DeliveryServiceName" type="HRTextType"/>
	<xsd:element name="TrackingID" type="HRIdentifierType"/>
	<xsd:element name="ShippingDate" type="FlexibleDateType"/>
	<xsd:element name="SpecimenAccessionID" type="HRIdentifierType">
		<xsd:annotation>
			<xsd:documentation>An identifier (usually a sequential number) assigned by the laboratory to the specimen. An "accession number" is so name based on a practice of recording or assigning the identifier based on the order of the acquisition in a manner to manage specimen custody and control.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="OfferDate" type="FlexibleDateType"/>
	<xsd:element name="OfferAcceptedDate" type="FlexibleDateType"/>
