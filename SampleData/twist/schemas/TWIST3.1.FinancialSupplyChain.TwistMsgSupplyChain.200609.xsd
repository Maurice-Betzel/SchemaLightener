			<xs:element name="otherProductNumber" minOccurs="0">
				<xs:annotation>
					<xs:documentation>added 0606-192</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="identifier" type="Identifier"/>
						<xs:element name="description">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="70"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="commodityCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>coded classification of the product from the relevant statistical commodity list or customs tariff. Also known as the statistical heading or tariff heading  added 0606-193</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="35"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="quotaCategoryCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>coded classification of the product quota purposes. added 0606-194</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="35"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="countryOfOrigin" type="Country" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The country in which the goods have been produced or manufactured. added 0606-195</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="manufacturerParty" type="PartyInformation" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Information about the party who manufactured the product added 0606-196</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="brandName" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>The brand name for the item added 0606-197</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="modelName" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>The model name for the item added 0606-198</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="lotNumber" type="Identifier" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The identification number assigned to a lot which includes the goods of the invoice line added 0606-199</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="lotExpiryDate" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The expiry date of the lot added 0606-200</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="productCharacteristic" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Identifies the characteristics of a product. added 0606-203</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="structuredCharacteristics" minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="typeCode" minOccurs="0">
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="15"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="value" minOccurs="0">
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="50"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="otherCharacteristics" minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="value" minOccurs="0">
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="50"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="description" minOccurs="0">
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="50"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TaxInformation">
		<xs:annotation>
			<xs:documentation>details the tax applicable to a individual line item [created 0606-160]</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="netPaymentAmount" type="Money" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The invoice line amount subject to a tax or fee</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxAmount" type="Money" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The sum of all tax and fee amounts valid for the invoice line</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxReferenceNumber" type="Identifier" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Tax identification number</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TransportDetails">
		<xs:annotation>
			<xs:documentation>created 0606-106</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="transportMode">
				<xs:annotation>
					<xs:documentation>transport mode represented as a code
list: {'1' - Sea | '2' - Rail | '3' - Road | '4' - Air | '6' - Multimodal | '8' - Inland water}
</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="1 - Sea"/>
						<xs:enumeration value="2 - Rail"/>
						<xs:enumeration value="3 - Road"/>
						<xs:enumeration value="4 - Air"/>
						<xs:enumeration value="6 - Multimodal"/>
						<xs:enumeration value="8 - Inland water"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="transportMeansType" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="code" minOccurs="0">
							<xs:annotation>
								<xs:documentation>type means of transport represented as a code</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="15"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="name" minOccurs="0">
							<xs:annotation>
								<xs:documentation>name of type of means of transport</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="35"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="transportMeansId" type="Identifier" minOccurs="0">
				<xs:annotation>
					<xs:documentation>identification of means of transport</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="placeOfDischarge" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Location where the goods are requested to be or were, discharged from the means of transport  [added 0606-213]</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="35"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SettlementDetail">
		<xs:sequence>
			<xs:element name="paymentDate" type="xs:date"/>
			<xs:element name="paymentAmount" type="Money" minOccurs="0"/>
			<xs:element name="paymentMethod" type="MethodOfPayment" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
