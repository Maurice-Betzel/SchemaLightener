	<xs:complexType name="FinanceRequestInvoiceDetail">
		<xs:sequence>
			<xs:element name="sequenceNumber" type="Identifier"/>
			<xs:element name="invoiceStatus" type="InvoiceStatusType"/>
			<xs:element name="invoiceStatusDate" type="xs:date" minOccurs="0"/>
			<xs:element name="financePlanNumber" type="Identifier" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Specifies the finance plan identification number</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="financePoolingIndicator" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates whether the referenced Invoice(s) are to be considered for Invoice pooling (offset against a redefined fixed finance amount) </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deliveryDate" type="xs:date" minOccurs="0"/>
			<xs:element name="invoiceCurrency" type="Currency" minOccurs="0"/>
			<xs:element name="paymentCurrency" type="Currency" minOccurs="0"/>
			<xs:element name="invoiceTotalTaxableAmount" type="Money" minOccurs="0">
				<xs:annotation>
					<xs:documentation>total amount subject to tax or fee.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceTotalTaxFeeAmount" type="Money" minOccurs="0">
				<xs:annotation>
					<xs:documentation>total of all tax and fee amounts.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceTotalAllowanceChargeAmount" type="Money" minOccurs="0">
				<xs:annotation>
					<xs:documentation>total of all allowance and charge amounts levied at the invoice level.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceTotalAmount" type="Money">
				<xs:annotation>
					<xs:documentation>total amount due, including invoice charges, allowances and taxes.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="requestedPayment" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="SettlementDetail">
							<xs:sequence>
								<xs:element name="exchangeRate" type="ExchangeRate" minOccurs="0"/>
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="invoiceReference" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="Invoice">
							<xs:sequence>
								<xs:element name="invoiceLineStatusDetail" type="LineStatusDetail" minOccurs="0" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
