				</xs:annotation>
			</xs:element>
			<xs:element name="latestDeliveryDate" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Latest delivery date/time allowed by the Buyer</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deliveryQuantity" type="Quantity" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Delivery quantity requested by the Buyer for this schedule entry</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="minimumQuantity" type="Quantity" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Minimum delivery quantity for this schedule entry</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="maximumQuantity" type="Quantity" minOccurs="0">
