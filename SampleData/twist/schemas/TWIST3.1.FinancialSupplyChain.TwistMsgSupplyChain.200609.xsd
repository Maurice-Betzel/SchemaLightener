<xs:schema xmlns="http://www.twiststandards.org/3.1/FinancialSupplyChain" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.twiststandards.org/3.1/FinancialSupplyChain" elementFormDefault="qualified" attributeFormDefault="unqualified">
