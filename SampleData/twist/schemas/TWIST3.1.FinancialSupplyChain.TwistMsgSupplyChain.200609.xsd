	<xs:complexType name="PaymentTerms">
		<xs:annotation>
			<xs:documentation>complex type for payment terms </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="invoiceDueDate" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The date the invoice payment is due</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="paymentDuePeriod" minOccurs="0">
				<xs:annotation>
					<xs:documentation>the number of days within which the invoice should be paid (changed name and type 0606-126,127)</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="dueDateTime" type="xs:dateTime" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The payment due date/time</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="durationMeasure" type="Measure" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Eg. the number of days within which the invoice should be paid</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="descriptionCode" minOccurs="0">
							<xs:annotation>
								<xs:documentation>A description of the payment period expressed as a code
list: {'EMTD (EndOfMonthOfDelivery)'|
'EMTR (EndOfMonthOfReceipt)'|
'EPRD (EndOfPeriodAfterDelivery)'|
'EPRR (EndOfPeriodAfterReceipt)'|
'IREC (PaymentOnReceiptOfInvoice)'|
'PRMD (EndOfPeriodAfterEndOfDeliveryMonth)'|
'PRMR (EndOfPeriodAfterEndOfReceiptMonth)}
</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="EMTD"/>
									<xs:enumeration value="EMTR"/>
									<xs:enumeration value="EPRD"/>
									<xs:enumeration value="EPRR"/>
									<xs:enumeration value="IREC"/>
									<xs:enumeration value="PRMD"/>
									<xs:enumeration value="PRMR"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="description" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>A text description of the payment period</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="discount" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Payment conditions that apply to be eligible for a discount [changed name and type 0606-128,129]</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="startDateTime" type="xs:dateTime" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The start date/time of the discount period</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="endDateTime" type="xs:dateTime" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The end date/time of the discount period</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="durationMeasure" type="Measure" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Eg. the number of days within which the payment should be paid to be eligible for a discount</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="descriptionCode" minOccurs="0">
							<xs:annotation>
								<xs:documentation>A description of the discount period expressed as a code</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="15"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="description" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>A text description of the discount period</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="discountPercentage" type="Rate" minOccurs="0">
							<xs:annotation>
								<xs:documentation>the percentage discount to be applied for early payment</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="discountAmount" type="Money" minOccurs="0">
							<xs:annotation>
								<xs:documentation>amount for discount (to be deducted) for early payment</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="paymentDiscount" type="DiscountType" minOccurs="0"/>
			<xs:element name="paymentTermsIdentifier" type="Identifier" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The identifier for the Payment Terms [added 0606-125]</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="penalty" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Payment conditions that apply to incur a penalty [added 0606-130]</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="startDateTime" type="xs:dateTime" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The start date/time of the penalty period</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="endDateTime" type="xs:dateTime" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The end date/time of the penalty period</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="durationMeasure" type="Measure" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Eg. the number of days within which the payment should be paid to be eligible for a penalty </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="descriptionCode" minOccurs="0">
							<xs:annotation>
								<xs:documentation>A description of the penalty period expressed as a code</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="15"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="description" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>A text description of the penalty period</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="penaltyPercentage" type="Rate" minOccurs="0">
							<xs:annotation>
								<xs:documentation>the percentage penalty to be applied for late payment</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="penaltyAmount" type="Money" minOccurs="0">
							<xs:annotation>
								<xs:documentation>amount for penalty (to be deducted) for late payment</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="paymentTermsClause" minOccurs="0">
							<xs:annotation>
								<xs:documentation>free text payment terms clause</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="35"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
