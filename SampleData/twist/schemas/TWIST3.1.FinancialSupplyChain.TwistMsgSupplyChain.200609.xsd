	<!-- Version 3.1 September 2006-->
	<!-- Specification Document: -->
	<xs:include schemaLocation="TWIST3.1.TWISTCore.TwistMsg.200609.xsd"/>
	<xs:complexType name="FinanceRequest">
		<xs:annotation>
			<xs:documentation>Message for requesting finance for an invoice(s) new with 3.1 add per 0906-17</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ResponseMessage">
				<xs:sequence>
					<xs:element name="financeRequestHeader" maxOccurs="unbounded">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="requestForFinanceNumber" type="Identifier">
									<xs:annotation>
										<xs:documentation>Unique number assigned by the issuer to identify a Request for Finance transaction  </xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="requestForFinanceDate" type="xs:date"/>
								<xs:element name="requestForFinanceType">
									<xs:annotation>
										<xs:documentation>Specifies the type of Request for Finance</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="code" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Request for Finance type represented as a code
list: {'PREB' – Pre-shipment  Buyer financing | 'PREV' – Pre-shipment Vendor financing | 'PSTB' – Post-shipment  Buyer financing | 'PSTV' – Post-shipment Vendor financing }
</xs:documentation>
												</xs:annotation>
												<xs:simpleType>
													<xs:restriction base="xs:string">
														<xs:enumeration value="PREB"/>
														<xs:enumeration value="PREV"/>
														<xs:enumeration value="PSTB"/>
														<xs:enumeration value="PSTV"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:element>
											<xs:element name="description" type="xs:string" minOccurs="0"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="GUID" type="Identifier" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Computer-generated globally unique identifier (GUID) for the transaction instance</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="instructions" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
								<xs:element name="financeRequestInvoiceDetail" type="FinanceRequestInvoiceDetail"/>
								<xs:element name="creditor">
									<xs:annotation>
										<xs:documentation>Information about the creditor</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:complexContent>
											<xs:extension base="PartyInformation"/>
										</xs:complexContent>
									</xs:complexType>
								</xs:element>
								<xs:element name="debtor">
									<xs:annotation>
										<xs:documentation>Information about the debtor</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:complexContent>
											<xs:extension base="PartyInformation"/>
										</xs:complexContent>
									</xs:complexType>
								</xs:element>
								<xs:element name="originator" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Information about the originator</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:complexContent>
											<xs:extension base="PartyInformation"/>
										</xs:complexContent>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="FinanceRequestCancellation">
		<xs:annotation>
			<xs:documentation>Message for cancelling a finance request for an invoice(s) new with 3.1 add per 0906-19</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ResponseMessage">
				<xs:sequence>
					<xs:element name="financeRequestCancellationHeader" minOccurs="0" maxOccurs="unbounded">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="financeRequestCancellationNumber" type="Identifier">
									<xs:annotation>
										<xs:documentation>Unique number assigned by the issuer to identify a Finance Request Cancellation transaction  </xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="financeRequestCancellationDate" type="xs:date">
									<xs:annotation>
										<xs:documentation>Date the Finance Request Cancellation transaction was issued</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="GUID" type="Identifier" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Computer-generated globally unique identifier (GUID) for the transaction instance</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="requestForFinanceNumber" type="Identifier">
									<xs:annotation>
										<xs:documentation>Unique number assigned by the issuer to identify a Request for Finance transaction  </xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="requestForFinanceDate" type="xs:date" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Date the Request for Finance transaction was issued</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="note" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
								<xs:element name="financeRequestCancellationDetail">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="sequenceNumber" type="Identifier">
												<xs:annotation>
													<xs:documentation>Unique sequence number </xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="invoiceStatus" type="InvoiceStatusType" minOccurs="0"/>
											<xs:element name="invoiceStatusDate" type="xs:date" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Date the Invoice Status transaction was issued</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="cancellationReason">
												<xs:complexType>
													<xs:sequence>
														<xs:element name="code" type="xs:string" minOccurs="0"/>
														<xs:element name="description" type="xs:string" minOccurs="0"/>
													</xs:sequence>
												</xs:complexType>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="creditor">
									<xs:annotation>
										<xs:documentation>Information about the creditor</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:complexContent>
											<xs:extension base="PartyInformation"/>
										</xs:complexContent>
									</xs:complexType>
								</xs:element>
								<xs:element name="debtor">
									<xs:annotation>
										<xs:documentation>Information about the debtor</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:complexContent>
											<xs:extension base="PartyInformation"/>
										</xs:complexContent>
									</xs:complexType>
								</xs:element>
								<xs:element name="originator" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Information about the originator</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:complexContent>
											<xs:extension base="PartyInformation"/>
										</xs:complexContent>
									</xs:complexType>
								</xs:element>
								<xs:element name="invoiceReference">
									<xs:complexType>
										<xs:complexContent>
											<xs:extension base="Invoice">
												<xs:sequence>
													<xs:element name="invoiceLineStatusDetail" type="LineStatusDetail" minOccurs="0" maxOccurs="unbounded"/>
												</xs:sequence>
											</xs:extension>
										</xs:complexContent>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="FinanceRequestStatusNotification">
		<xs:annotation>
			<xs:documentation>Message for requesting status of a finance request for an invoice(s) new with 3.1 add per 0906-18</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ResponseMessage">
				<xs:sequence>
					<xs:element name="financeRequestStatusNotificationHeader" maxOccurs="unbounded">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="financeRequestStatusNotificationNumber" type="Identifier">
									<xs:annotation>
										<xs:documentation>Unique number assigned by the issuer to identify a Finance Request Status Notification transaction  </xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="financeRequestStatusNotificationDate" type="xs:date">
									<xs:annotation>
										<xs:documentation>Date the Finance Request Status Notification transaction was issued</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="financeRequestStatus">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="code" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Request for Finance status represented as a code
list: {'PND' – Pending | 'APP' – Approved | 'REJ' – Rejected | 'CAN' – Cancelled }
</xs:documentation>
												</xs:annotation>
												<xs:simpleType>
													<xs:restriction base="xs:string">
														<xs:enumeration value="PND"/>
														<xs:enumeration value="APP"/>
														<xs:enumeration value="REJ"/>
														<xs:enumeration value="CAN"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:element>
											<xs:element name="description" type="xs:string" minOccurs="0"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="GUID" type="Identifier" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Computer-generated globally unique identifier (GUID) for the transaction instance</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="requestForFinanceNumber" type="Identifier">
									<xs:annotation>
										<xs:documentation>Unique number assigned by the issuer to identify a Request for Finance transaction  </xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="requestForFinanceDate" type="xs:date">
									<xs:annotation>
										<xs:documentation>Date the Request for Finance transaction was issued</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="note" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
								<xs:element name="creditor">
									<xs:annotation>
										<xs:documentation>Information about the creditor</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:complexContent>
											<xs:extension base="PartyInformation"/>
										</xs:complexContent>
									</xs:complexType>
								</xs:element>
								<xs:element name="debtor">
									<xs:annotation>
										<xs:documentation>Information about the debtor</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:complexContent>
											<xs:extension base="PartyInformation"/>
										</xs:complexContent>
									</xs:complexType>
								</xs:element>
								<xs:element name="originator" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Information about the originator</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:complexContent>
											<xs:extension base="PartyInformation"/>
										</xs:complexContent>
									</xs:complexType>
								</xs:element>
								<xs:element name="financeRequestInvoiceDetail" maxOccurs="unbounded">
									<xs:complexType>
										<xs:complexContent>
											<xs:extension base="FinanceRequestInvoiceDetail">
												<xs:sequence>
													<xs:element name="financedPayment" minOccurs="0" maxOccurs="unbounded">
														<xs:complexType>
															<xs:complexContent>
																<xs:extension base="SettlementDetail">
																	<xs:sequence>
																		<xs:element name="exchangeRate" type="ExchangeRate" minOccurs="0"/>
																	</xs:sequence>
																</xs:extension>
															</xs:complexContent>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:extension>
										</xs:complexContent>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="Invoice">
		<xs:annotation>
			<xs:documentation>Message for an invoice</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="RequestMessage">
				<xs:sequence>
					<xs:element name="invoiceHeader" maxOccurs="unbounded">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="invoiceNumber" type="Identifier" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Unique identification of the invoice (from the vendor's system) [renamed invoiceNumber and type to Identifier (0606-77)]</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="invoiceDate" type="xs:date">
									<xs:annotation>
										<xs:documentation>The date the invoice was issued</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="invoicePeriodStartDate" type="xs:dateTime" minOccurs="0">
									<xs:annotation>
										<xs:documentation>[added 0606-78)]</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="invoicePeriodEndDate" type="xs:dateTime" minOccurs="0">
									<xs:annotation>
										<xs:documentation>[added 0606-79)]</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="GUID" type="GUID" minOccurs="0">
									<xs:annotation>
										<xs:documentation>[added (0606-80)]</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="buyerInvoiceReferenceNumber" type="Identifier" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Unique identification of the invoice (from the buyer's system) [changed name from buyerInvoiceNumber to buyerInvoiceReferenceNumber and type to Identifier and 1-1 to 0-1 (0606-81)]</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="sellerInvoiceReferenceNumber" type="Identifier" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Identification number assigned to the invoice by the seller -added 0606-239</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="relatedInvoiceNumber" type="DocumentReference" minOccurs="0">
									<xs:annotation>
										<xs:documentation>For related documents case e.g. credit note/prepayment invoice is attached to a specific invoice) [changed type to DocumentReference -0606-85]</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="invoiceAttachment" type="Attachment" minOccurs="0" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>File attachments for an invoice including the binary attachment, file url - could be scan of invoice supporting documents</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="invoiceCurrency" type="Currency" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Currency for the total amount of the invoice</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="paymentCurrency" type="Currency" minOccurs="0">
									<xs:annotation>
										<xs:documentation>The coded identifier of the monetary unit to be used for the payment of an invoice.
list: {'AED'|'AFN'|'ALL'… etc.  ISO 4217 currency codes</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="buyerInformation" type="PartyInformation">
									<xs:annotation>
										<xs:documentation>Information about the buyer added 0606-88</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="sellerInformation" type="PartyInformation">
									<xs:annotation>
										<xs:documentation>Information about the seller added 0606-89</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="creditorInformation" type="PartyInformation" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Information about the creditor  [changed to (0-1) 0606-90)</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="debtorInformation" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Information about the debtor  [changed to (0-1) 0606-91] </xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:complexContent>
											<xs:extension base="PartyInformation"/>
										</xs:complexContent>
									</xs:complexType>
								</xs:element>
								<xs:element name="remitToInformation" type="PartyInformation" minOccurs="0">
									<xs:annotation>
										<xs:documentation>In the case of factoring, the payment is remitted to the factor, not the bill from.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="consigneeInformation" type="PartyInformation" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Information about the party to whom the goods must be delivered, if different from buyer added 0606-92</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="paymentTerms" type="PaymentTerms">
									<xs:annotation>
										<xs:documentation>Conditions applicable to the invoice payment</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="paymentMethod" type="MethodOfPayment" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Indication of the preferred financial instrument to effect payment added 0606-93</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="invoiceType">
									<xs:annotation>
										<xs:documentation>Type of invoice: commercial,consolidated,pro forma etc. [updated enumeration list 0606-222]</xs:documentation>
									</xs:annotation>
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:enumeration value="Proforma invoice"/>
											<xs:enumeration value="Commercial invoice"/>
											<xs:enumeration value="Credit note"/>
											<xs:enumeration value="Debit note"/>
											<xs:enumeration value="Commission note"/>
											<xs:enumeration value="Consolidated invoice"/>
											<xs:enumeration value="Prepayment invoice"/>
											<xs:enumeration value="Self-billed invoice"/>
											<xs:enumeration value="Delcredere invoice"/>
											<xs:enumeration value="Factored invoice"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:element>
								<xs:element name="invoiceFunction">
									<xs:annotation>
										<xs:documentation>Function of invoice
list: {'Original'|'Copy'|'Duplicate'|'Cancellation'|'Retransmission'
|'Test'|'Confirmation'|'Accepted for financing'}
 added 0606-94</xs:documentation>
									</xs:annotation>
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:enumeration value="Original"/>
											<xs:enumeration value="Copy"/>
											<xs:enumeration value="Duplicate"/>
											<xs:enumeration value="Cancellation"/>
											<xs:enumeration value="Retransmission"/>
											<xs:enumeration value="Test"/>
											<xs:enumeration value="Confirmation"/>
											<xs:enumeration value="Accepted for Financing"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:element>
								<xs:element name="taxExemptionAuthority" minOccurs="0">
									<xs:annotation>
										<xs:documentation>reference to tax authority that granted the taxation exemption</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="exemptionNumber">
												<xs:simpleType>
													<xs:restriction base="xs:string">
														<xs:length value="30"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:element>
											<xs:element name="exemptionGrantedDate" type="xs:date"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="taxPointDate" type="xs:dateTime" minOccurs="0">
									<xs:annotation>
										<xs:documentation>The date on which the supply of goods or services was made / completed for VAT purposes, or the date on which any payment on account was made, where this date can be determined and differs from the issue date of the invoice. The tax point date is valid for all invoice lines. added 0606-95</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="invoiceTotalLineAmount" type="Money" minOccurs="0">
									<xs:annotation>
										<xs:documentation>total of all line items (invoiceLineAmount), including line item charges and allowances added 0606-96</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="invoiceTotalTaxableAmount" type="Money" minOccurs="0">
									<xs:annotation>
										<xs:documentation>total amount subject to tax or fee. added 0606-97</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="invoiceTotalTaxFeeAmount" type="Money" minOccurs="0">
									<xs:annotation>
										<xs:documentation>total of all tax and fee amounts. added 0606-98</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="invoiceTotalAllowanceChargeAmount" type="Money" minOccurs="0">
									<xs:annotation>
										<xs:documentation>total of all allowance and charge amounts levied at the invoice level. added 0606-99</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="invoiceTotalAmount" type="Money" minOccurs="0">
									<xs:annotation>
										<xs:documentation>total amount due, including invoice charges, allowances and taxes. added 0606-100</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="exchangeRate" minOccurs="0">
									<xs:annotation>
										<xs:documentation>exchange rate used for the conversion of the invoicing currency from a source currency added 0606-101</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:complexContent>
											<xs:extension base="ExchangeRate">
												<xs:sequence>
													<xs:element name="date" type="xs:date" minOccurs="0"/>
												</xs:sequence>
											</xs:extension>
										</xs:complexContent>
									</xs:complexType>
								</xs:element>
								<xs:element name="deliveryTerms" type="DeliveryTerms" minOccurs="0">
									<xs:annotation>
										<xs:documentation>details concerning the terms of delivery added 0606-104</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="documentReference" type="DocumentReference" minOccurs="0" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>document reference added 0606-105 </xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="transportDetails" type="TransportDetails" minOccurs="0">
									<xs:annotation>
										<xs:documentation>specific details concerning the transport used to ship the goods  adeed 0606-107</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="consignmentDetails" type="ConsignmentDetails" minOccurs="0" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>specific details concerning the consignment  added 0606-109</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="allowanceCharge" type="AllowanceCharges" minOccurs="0" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>specific charges and discounts applied at the Invoice level, e.g. Packing Charges, Freight Charges, Insurance Charges, Other Fees, Discount, etc. Exclusive of any charges or discounts applied at the line item level.  [added 0606-225]</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="exemptionNumber" minOccurs="0">
									<xs:annotation>
										<xs:documentation>official exemption number granted by the tax authority </xs:documentation>
									</xs:annotation>
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:maxLength value="30"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:element>
								<xs:element name="invoiceDetail" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>Details of the invoice by line</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="lineItemNumber" type="LineNumber">
												<xs:annotation>
													<xs:documentation>unique number of the invoice line item</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="lineItemType" type="xs:string" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Specifies the type of invoice line item by means of a code
list: {‘LIN’ - Normal line item | ‘SUB’ - Sub-line item | ‘TOT’ - Subtotal  line item}
added 0606-241</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="GUID" minOccurs="0">
												<xs:annotation>
													<xs:documentation>A computer-generated globally unique identifier (GUID) for the Invoice Line instance [added 0606-153]</xs:documentation>
												</xs:annotation>
												<xs:simpleType>
													<xs:restriction base="GUID">
														<xs:enumeration value="LIN"/>
														<xs:enumeration value="SUB"/>
														<xs:enumeration value="TOT"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:element>
											<xs:element name="itemPriceTotal" type="Money">
												<xs:annotation>
													<xs:documentation>total price information for the invoice item (calculated as pricePerUnit x quantityInvoiced), i.e. without any allowances, charges or taxes</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="quantityInvoiced" type="xs:decimal">
												<xs:annotation>
													<xs:documentation>quantity of product units invoiced and
associated unit of measurement</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="invoicePeriodStartDate" type="xs:dateTime" minOccurs="0">
												<xs:annotation>
													<xs:documentation>invoice period start date, used for period invoicing [added 0606-154</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="invoicePeriodEndDate" type="xs:dateTime" minOccurs="0">
												<xs:annotation>
													<xs:documentation>invoice period end date, used for period invoicing[added 0606-155]</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="userDefinedDescription" type="xs:string">
												<xs:annotation>
													<xs:documentation>Remarks for the item</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="productItem" type="ProductItem">
												<xs:annotation>
													<xs:documentation>product details added 0606-184</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="pricePerUnit" type="Money">
												<xs:annotation>
													<xs:documentation>price per unit of product invoiced</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="priceUnit" minOccurs="0">
												<xs:annotation>
													<xs:documentation>quantity and unit of measure to which the pricePerUnit applies [added 0606-156]</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:sequence>
														<xs:element name="quantity" type="Quantity" minOccurs="0"/>
														<xs:element name="unitOfMeasure" minOccurs="0">
															<xs:simpleType>
																<xs:restriction base="xs:string">
																	<xs:maxLength value="35"/>
																</xs:restriction>
															</xs:simpleType>
														</xs:element>
													</xs:sequence>
												</xs:complexType>
											</xs:element>
											<xs:element name="invoiceLineAmount" type="Money">
												<xs:annotation>
													<xs:documentation>Calculated net amount (price plus charges, minus allowances, plus taxes) of the invoice line item</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="taxInformation" type="TaxInformation" minOccurs="0" maxOccurs="unbounded">
												<xs:annotation>
													<xs:documentation>details the tax applicable to a individual line item [name changed from vATInformation 0606-158, changed from 1-1 to 0-n 0606-159 changed type from  TaxInfo to TaxInformation 0606-160b]</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="allowanceCharge" type="AllowanceCharges" minOccurs="0" maxOccurs="unbounded">
												<xs:annotation>
													<xs:documentation>charges amount included for total of invoice [changed invoiceCharges to allowanceCharge 0-n 0606-162]</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="deliveryDate" type="xs:dateTime" minOccurs="0">
												<xs:annotation>
													<xs:documentation>The date when the goods line item was delivered to the buyer added 0606-163</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="despatchDate" type="xs:dateTime" minOccurs="0">
												<xs:annotation>
													<xs:documentation>The date when the goods line item was despatched by the supplier added 0606-164</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="grossWeightMeasure" type="Measure" minOccurs="0">
												<xs:annotation>
													<xs:documentation>total consignment gross weight (includes packaging) added 0606-165</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="netWeightMeasure" type="Measure" minOccurs="0">
												<xs:annotation>
													<xs:documentation>total consignment net weight (excludes all packaging) added 0606-166</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="netNetWeightMeasure" type="Measure" minOccurs="0">
												<xs:annotation>
													<xs:documentation>total consignment net net weight (excludes all packaging and conversion material) added 0606-167</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="grossVolumeMeasure" type="Measure" minOccurs="0">
												<xs:annotation>
													<xs:documentation>total consignment volume added 0606-168</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="FOBValueAmount" type="Money" minOccurs="0">
												<xs:annotation>
													<xs:documentation>line item FOB (FreeOnBoard) amount added 0606-169</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="CIFValueAmount" type="Money" minOccurs="0">
												<xs:annotation>
													<xs:documentation>line item CIF (CostInsuranceAndFreight) amount added 0606-170</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="CFRValueAmount" type="Money" minOccurs="0">
												<xs:annotation>
													<xs:documentation>line item CFR (CostAndFreight) amount added 0606-171</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="freeOfChargeIndicator" type="xs:boolean" minOccurs="0">
												<xs:annotation>
													<xs:documentation>indicates whether line item goods are free of charge 1=freeOfCharge [added 0606-172]</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="purchaseOrderNumber" type="DocumentReference" minOccurs="0">
												<xs:annotation>
													<xs:documentation>purchase order identification number by which the goods were purchased.  [Changed from OrderNumber type to DocumentReference 0606-173 changed from 1-1 to 0-1 0606-176]</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="purchaseOrderLineNumber" type="Identifier" minOccurs="0">
												<xs:annotation>
													<xs:documentation>line number on the purchase order changed to Identifier and from 1-1 to 0-1 0606-174</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="purchaseOrderLineStatusCode" minOccurs="0">
												<xs:annotation>
													<xs:documentation>status of the purchase order line added 0606-175</xs:documentation>
												</xs:annotation>
												<xs:simpleType>
													<xs:restriction base="xs:string">
														<xs:maxLength value="15"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:element>
											<xs:element name="purchaseOrderLineGUID" type="GUID" minOccurs="0">
												<xs:annotation>
													<xs:documentation>a computer-generated globally unique identifier (GUID) for the referenced purchase order line [added 0606-177]</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="deliverySequenceNumber" minOccurs="0">
												<xs:annotation>
													<xs:documentation>identification of order by which the goods were delivered [changed name from scheduleNumber to deliverySequenceNumber 0606-178]</xs:documentation>
												</xs:annotation>
												<xs:simpleType>
													<xs:restriction base="xs:integer">
														<xs:totalDigits value="3"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:element>
											<xs:element name="contractId" type="DocumentReference" minOccurs="0">
												<xs:annotation>
													<xs:documentation>unique identifier for the buying agreement [changed type to DocumentReference 0606-179]</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="otherDocumentReference" type="DocumentReference" minOccurs="0" maxOccurs="unbounded">
												<xs:annotation>
													<xs:documentation>other document reference not already specified for line item added 0606-180</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="industrySpecificDetails" minOccurs="0">
												<xs:annotation>
													<xs:documentation>additional information</xs:documentation>
												</xs:annotation>
												<xs:simpleType>
													<xs:restriction base="xs:string">
														<xs:maxLength value="100"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="invoiceAdjustment" type="Adjustment" minOccurs="0" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>Adjustment details related to the credit note, debit note or invoice added 0606-123</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="note" type="xs:string" minOccurs="0" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>added 0606-124</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="InvoiceAcceptance">
		<xs:annotation>
			<xs:documentation>Message to notify of invoice acceptance new with 3.1 added per 0906-</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ResponseMessage">
				<xs:sequence>
					<xs:element name="invoiceAcceptanceHeader" maxOccurs="unbounded">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="invoiceAcceptanceDate" type="xs:date">
									<xs:annotation>
										<xs:documentation>Date the Invoice Acceptance transaction was issued</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="GUID" type="Identifier" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Computer-generated globally unique identifier (GUID) for the transaction instance</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="instructions" type="xs:string" minOccurs="0" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>Free form text instructions to the transaction recipient</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="buyer">
									<xs:annotation>
										<xs:documentation>Information about the buyer</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:complexContent>
											<xs:extension base="PartyInformation"/>
										</xs:complexContent>
									</xs:complexType>
								</xs:element>
								<xs:element name="originator" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Information about the originator</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:complexContent>
											<xs:extension base="PartyInformation"/>
										</xs:complexContent>
									</xs:complexType>
								</xs:element>
								<xs:element name="InvoiceAcceptanceDetail">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="sequenceNumber" type="Identifier"/>
											<xs:element name="invoiceStatus" type="InvoiceStatusType"/>
											<xs:element name="invoiceStatusDate" type="xs:date" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Date the Invoice Status transaction was issued</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="financePlanNumber" type="Identifier" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Specifies the finance plan identification number</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="seller">
												<xs:annotation>
													<xs:documentation>Information about the seller</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:complexContent>
														<xs:extension base="PartyInformation"/>
													</xs:complexContent>
												</xs:complexType>
											</xs:element>
											<xs:element name="documentReference" minOccurs="0" maxOccurs="unbounded">
												<xs:complexType>
													<xs:complexContent>
														<xs:extension base="DocumentReference">
															<xs:sequence>
																<xs:element name="invoiceLineStatusDetail" type="LineStatusDetail" minOccurs="0" maxOccurs="unbounded"/>
															</xs:sequence>
														</xs:extension>
													</xs:complexContent>
												</xs:complexType>
											</xs:element>
											<xs:element name="invoiceDocument" minOccurs="0" maxOccurs="unbounded">
												<xs:complexType>
													<xs:complexContent>
														<xs:extension base="Invoice">
															<xs:sequence>
																<xs:element name="invoiceLineStatusDetail" type="LineStatusDetail" minOccurs="0" maxOccurs="unbounded"/>
															</xs:sequence>
														</xs:extension>
													</xs:complexContent>
												</xs:complexType>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="invoiceAcceptanceNumber" type="Identifier">
									<xs:annotation>
										<xs:documentation>Unique number assigned by the issuer to identify a Invoice Acceptance transaction  </xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="InvoiceDispute">
		<xs:annotation>
			<xs:documentation>Message for an invoice dispute - DEPRECATED WITH RELEASE 3.1 - BEING REPLACED BY INVOICE STATUS - 200609</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="RequestMessage">
				<xs:sequence>
					<xs:element name="disputeHeader" maxOccurs="unbounded">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="disputeNumber">
									<xs:annotation>
										<xs:documentation>unique identification number of the dispute message</xs:documentation>
									</xs:annotation>
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:maxLength value="20"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:element>
								<xs:element name="buyerInvoiceNumber" type="OrderNumber">
									<xs:annotation>
										<xs:documentation>Unique identification of the invoice (from the buyer's system)</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="disputedAmount" type="Money">
									<xs:annotation>
										<xs:documentation>Total amount that is subject to dispute</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="debtorInformation">
									<xs:annotation>
										<xs:documentation>Information about the debtor </xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:complexContent>
											<xs:extension base="PartyInformation"/>
										</xs:complexContent>
									</xs:complexType>
								</xs:element>
								<xs:element name="vendorInvoiceNumber" type="OrderNumber" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Unique identification of the invoice (from the vendor's system) </xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="creditorInformation" type="PartyInformation">
									<xs:annotation>
										<xs:documentation>Information about the creditor</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="genericDispute">
									<xs:annotation>
										<xs:documentation>indicator to express that the invoice is rejected in total, e.g sent to the wrong debtor </xs:documentation>
									</xs:annotation>
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:maxLength value="20"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:element>
								<xs:element name="disputeDetail" maxOccurs="unbounded">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="disputedInvoiceLine" type="LineNumber">
												<xs:annotation>
													<xs:documentation>unique identifier of the disputed line of the invoice</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="disputeType" type="xs:string">
												<xs:annotation>
													<xs:documentation>the nature of the dispute e.g. incorrect quantity</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="invoiceData" type="xs:string">
												<xs:annotation>
													<xs:documentation>original data on the invoice that is in dispute - quantity, unit price etc.</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="disputeData" type="xs:string">
												<xs:annotation>
													<xs:documentation>expected data on the invoice that is in dispute - quantity, unit price etc. </xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="disputedAmountNet" type="Money">
												<xs:annotation>
													<xs:documentation>financial impact of the dispute, the difference between the original invoice NET and the adjusted value</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="disputedVATAmount" type="Money">
												<xs:annotation>
													<xs:documentation>financial impact of the dispute, difference original invoice GROSS amount for that line and adjusted value</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="disputeResolution" type="xs:string">
												<xs:annotation>
													<xs:documentation>suggested way to resolve the dispute, eg by credit note, payment adjustment - source</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="InvoiceStatus">
		<xs:annotation>
			<xs:documentation>Message for information about an invoice including disputes new with 3.1 added per 0906-14</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ResponseMessage">
				<xs:sequence>
					<xs:element name="invoiceStatusHeader" maxOccurs="unbounded">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="invoiceStatusDate" type="xs:date">
									<xs:annotation>
										<xs:documentation>Date the Invoice Status transaction was issued</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="GUID" type="Identifier" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Computer-generated globally unique identifier (GUID) for the transaction instance</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="invoiceStatus" type="InvoiceStatusType"/>
								<xs:element name="invoiceRejectionReason" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Specifies the reason for rejection</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="code" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Rejection reason represented as a code
list: {'DUP' – Duplicate transaction | 'DIS' – Disputed transaction}
</xs:documentation>
												</xs:annotation>
												<xs:simpleType>
													<xs:restriction base="xs:string">
														<xs:enumeration value="DUP"/>
														<xs:enumeration value="DIS"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:element>
											<xs:element name="description" type="xs:string" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Description of the rejection reason</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="invoiceResolutionResult" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Specifies the dispute resolution result</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="code" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Resolution result represented as a code
list: {'ACC' – Accepted without change | 'AMD' – Accepted subject to change | ‘RES’ – Resubmitted}	0..1</xs:documentation>
												</xs:annotation>
												<xs:simpleType>
													<xs:restriction base="xs:string">
														<xs:enumeration value="ACC"/>
														<xs:enumeration value="AMD"/>
														<xs:enumeration value="RES"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:element>
											<xs:element name="description" type="xs:string" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Description of the rejection reason</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="note" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
