	<xs:complexType name="InvoiceStatusType">
		<xs:sequence>
			<xs:element name="code" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Invoice status represented as a code
list: {'ANS' - Approved For Normal Settlement | 'ADS' - Approved For Vendor Discounted Settlement | 'AFF' - Approved For Finance | 'REJ' - Rejected |  'PAP' - PartApproved | 'PNS' - PartApproved For Normal Settlement | 'PDS' - PartApproved For Supplier Discounted Settlement | 'PFF' - PartApproved For Finance}	0..1</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="ANS"/>
						<xs:enumeration value="ADS"/>
						<xs:enumeration value="AFF"/>
						<xs:enumeration value="REJ"/>
						<xs:enumeration value="PAP"/>
						<xs:enumeration value="PNS"/>
						<xs:enumeration value="PDS"/>
						<xs:enumeration value="PFF"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Description" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Description of the invoice status</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ItemQuantity">
		<xs:annotation>
			<xs:documentation>created to support multiple use in LineItem complex type</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="quantity" type="Quantity"/>
			<xs:element name="unitOfMeasure">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="35"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="LineItem">
		<xs:annotation>
			<xs:documentation>created per 0606-211</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="lineItemNumber" type="Identifier">
				<xs:annotation>
					<xs:documentation>Unique number of the transaction line item</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="lineItemTypeCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Specifies the type line item by means of a code list: {‘LIN’ - Normal line item | ‘SBX’ – Substitute line item | ‘RPX’ - Replacement  line item}</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="LIN"/>
						<xs:enumeration value="SBX"/>
						<xs:enumeration value="RPX"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="lineItemStatusCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Specifies the status of the line item by means of a code list: {‘ACP’ - Accepted by Seller| ‘REJ’ – Rejected by Seller | ‘PND’ – Pending by Seller | ‘ACW’ - Accepted without change by Seller | ‘ACX’ - Accepted with change by Seller | ‘DEL’ – Deleted by Buyer | ‘ADD’ – Added by Buyer| ‘CHG’ – Changed by Buyer}</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="ACP"/>
						<xs:enumeration value="REJ"/>
						<xs:enumeration value="PND"/>
						<xs:enumeration value="ACW"/>
						<xs:enumeration value="ACX"/>
						<xs:enumeration value="DEL"/>
						<xs:enumeration value="ADD"/>
						<xs:enumeration value="CHG"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="GUID" type="GUID" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Computer-generated globally unique identifier (GUID) for the entity (Order, Order line item, etc) instance</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="lineItemQuantity" type="ItemQuantity" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Quantity of product units and associated unit of measurement</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="lineItemAmount" type="Money" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Calculated net amount (price plus charges, minus allowances, plus taxes) of the line item</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="lineItemTaxableAmount" type="Money" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Line item amount subject to a tax or fee</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="lineItemTaxFeeAmount" type="Money" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Line item tax or fee amount</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="minimumQuantity" type="ItemQuantity" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Line item minimum supply quantity</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="maximumQuantity" type="ItemQuantity" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Line item maximum supply quantity</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="minimumBackOrderQuantity" type="ItemQuantity" minOccurs="0">
				<xs:annotation>
					<xs:documentation>minimumBackOrderQuantity</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="maximumBackOrderQuantity" type="ItemQuantity" minOccurs="0">
				<xs:annotation>
					<xs:documentation>maximumBackOrderQuantity</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="partialShipmentAllowedIndicator" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indication of whether the buyer allows partial shipment of the ordered goods 0=no, 1=yes</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="backOrderAllowedIndicator" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indication of whether the buyer allows the ordered goods to placed on back order, partially or fully 0=no, 1=yes</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="priceAmount" type="Money" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Price of the line item, calculated as pricePerUnit x lineItemQuantity, i.e. without any allowances, charges or taxes</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="pricePerUnit" type="Money" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Price per unit of product </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="priceUnit" type="ItemQuantity" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Quantity and unit of measure to which the pricePerUnit applies</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="productItem" type="ProductItem">
				<xs:annotation>
					<xs:documentation>Product details</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="allowanceCharge" type="AllowanceCharges" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Individual line item allowance or charge amount</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="quoteReference" type="DocumentReference" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Reference to the related Quotation </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="contractReference" type="DocumentReference" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Reference to the buying agreement</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="otherDocumentReference" type="DocumentReference" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Other document reference not already specified for line item</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="packagingDetails" type="PackagingDetails" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Information concerning the packaging of the line item</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="LineStatusDetail">
		<xs:sequence>
			<xs:element name="lineItemNumber" type="Identifier">
				<xs:annotation>
					<xs:documentation>Unique number of the transaction line item</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GUID" type="Identifier" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Computer-generated globally unique identifier (GUID) for the line item instance</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="lineItemStatusCode">
				<xs:annotation>
					<xs:documentation>Specifies the status of the line item by means of a code
list: {‘ACP’ - Accepted by Seller| ‘REJ’ – Rejected by Seller | ‘PND’ – Pending by Seller | ‘ACW’ - Accepted without change by Seller | ‘ACX’ - Accepted with change by Seller | ‘DEL’ – Deleted by Buyer | ‘ADD’ – Added by Buyer| ‘CHG’ – Changed by Buyer | 'ANS' - Approved For Normal Settlement | 'ADS' - Approved For Vendor Discounted Settlement | 'AFF' - Approved For Finance }
</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="ACP"/>
						<xs:enumeration value="REJ"/>
						<xs:enumeration value="PND"/>
						<xs:enumeration value="ACW"/>
						<xs:enumeration value="ACX"/>
						<xs:enumeration value="DEL"/>
						<xs:enumeration value="ADD"/>
						<xs:enumeration value="CHG"/>
						<xs:enumeration value="ANS"/>
						<xs:enumeration value="ADS"/>
						<xs:enumeration value="AFF"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="lineRejectionReason" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Specifies the reason for rejection</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="code" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Rejection reason represented as a code
list: {'DIS' – Disputed line }
</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="DIS"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="description" type="xs:string" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="lineResolutionResult" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Specifies the dispute resolution result</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="code" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Resolution result represented as a code
list: {'ACC' – Accepted without change | 'AMD' – Accepted subject to change | ‘RES’ – Resubmitted}	0..1</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="ACC"/>
									<xs:enumeration value="AMD"/>
									<xs:enumeration value="RES"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="description" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Description of the rejection reason</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="disputeDetail" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="disputeType" type="xs:string">
							<xs:annotation>
								<xs:documentation>the nature of the dispute e.g. incorrect quantity</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="invoiceData" type="xs:string">
							<xs:annotation>
								<xs:documentation>original data on the invoice that is in dispute - quantity, unit price etc.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="disputeData" type="xs:string">
							<xs:annotation>
								<xs:documentation>expected data on the invoice that is in dispute - quantity, unit price etc. </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="disputedAmountNet" type="Money">
							<xs:annotation>
								<xs:documentation>financial impact of the dispute, the difference between the original invoice NET and the adjusted value</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="disputedVATAmount" type="Money">
							<xs:annotation>
								<xs:documentation>financial impact of the dispute, difference original invoice GROSS amount for that line and adjusted value</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="disputeResolution" type="xs:string">
							<xs:annotation>
								<xs:documentation>suggested way to resolve the dispute, eg by credit note, payment adjustment - source</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Location">
		<xs:annotation>
			<xs:documentation>created per 0606-84</xs:documentation>
		</xs:annotation>
		<xs:sequence>
