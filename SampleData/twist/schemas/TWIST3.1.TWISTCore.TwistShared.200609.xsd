<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<!-- Copyright 2006 TWIST Process Innovations, LTD. -->
	<!--Working Draft-->
	<!-- This schema is available for comment and the structure is subject to change -->
	<!-- Version 3.1 September 2006-->
	<!-- Specification Document: -->
	<xs:complexType name="Address">
		<xs:annotation>
			<xs:documentation>can be imported from fpml-shared  included elements and data types can be removed</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="streetAddress" type="StreetAddress" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="city" type="City" minOccurs="0"/>
			<xs:element name="state" type="xs:string" minOccurs="0"/>
			<xs:element name="country" type="Country" minOccurs="0"/>
			<xs:element name="postalCode" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AdjustableDate">
		<xs:sequence>
			<xs:element name="unadjustedDate" type="xs:date"/>
			<xs:element name="dateAdjustments" type="BusinessDayAdustments"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BankAccountNumber">
		<xs:choice>
			<xs:element name="iBAN" type="IBAN"/>
			<xs:element name="bBAN" type="BBAN"/>
			<xs:element name="uPIC" type="UPIC"/>
			<xs:element name="domesticId">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="35"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="BankAccountNumberInfo">
		<xs:sequence>
			<xs:element name="bankAccountType" type="BankAccountType" minOccurs="0"/>
			<xs:element name="bankAccountNumber" type="BankAccountNumber"/>
			<xs:element name="bankAccountCCY" type="Currency"/>
			<xs:element name="bankAccountName">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="70"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BankInfo">
		<xs:sequence>
			<xs:element name="bankId" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="35"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="bankRef" type="BankRef" minOccurs="0"/>
			<xs:element name="name" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="70"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="bankBranchId" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="35"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="bankBranchName" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="70"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="postAddress" type="PostAddress" minOccurs="0">
				<xs:annotation>
					<xs:documentation>post address of the bank</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="branchPostAddress" type="PostAddress" minOccurs="0"/>
			<xs:element name="clearingSystemId" type="ClearingSystemId" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BankRef">
		<xs:sequence>
			<xs:element name="bankIdType" type="BankIdType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BeneficiaryInformation">
		<xs:sequence>
			<xs:element name="beneficiaryBank" type="Routing" minOccurs="0"/>
			<xs:element name="beneficiary" type="Routing"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Block">
		<xs:sequence>
			<xs:element name="blockId" type="BlockId"/>
			<xs:element name="blockConstraint" type="BlockConstraint" minOccurs="0"/>
			<xs:element name="partialBlock" type="xs:boolean" minOccurs="0"/>
			<xs:element name="blockItem" type="BlockItem" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BlockItem">
		<xs:sequence>
			<xs:element name="partyTradeIdentifier" type="PartyTradeIdentifier" maxOccurs="unbounded"/>
			<xs:element name="blockPartyReference" type="PartyReference" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This reference the party to use on a single blockIetm.  If not present then the party from the parent trade is used.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="financialTerms" type="FinancialTerms"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BusinessCenters">
		<xs:sequence>
			<xs:element name="businessCenter" type="xs:string" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="id" type="xs:ID" use="required"/>
	</xs:complexType>
	<xs:complexType name="BusinessCenterTime">
		<xs:sequence>
			<xs:element name="hourMinuteTime" type="xs:string"/>
			<xs:element name="businessCenter" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BusinessDayAdustments">
		<xs:sequence>
			<xs:element name="businessDayConvention" type="xs:string"/>
			<xs:choice minOccurs="0">
				<xs:element name="businessCentersReference">
					<xs:complexType>
						<xs:attribute name="href" type="xs:normalizedString" use="required"/>
					</xs:complexType>
				</xs:element>
				<xs:element name="businessCenters" type="BusinessCenters"/>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CalculationAgent">
		<xs:choice>
			<xs:element name="calculationAgentPartyReference" type="PartyReference" maxOccurs="unbounded"/>
			<xs:element name="calculationAgentParty" type="CalculationAgentParty"/>
		</xs:choice>
		<xs:attribute name="href" type="xs:IDREF" use="required"/>
	</xs:complexType>
	<xs:complexType name="ContactInfo">
		<xs:sequence>
			<xs:element name="departmentName" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Department name [added 0606-151]</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="email" type="Email" minOccurs="0"/>
			<xs:element name="fax" type="PhoneNumber" minOccurs="0"/>
			<xs:element name="otherCommunication" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Other contact communication channel [added 0606-152]</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="channelCode">
							<xs:annotation>
								<xs:documentation>The method of communication expressed as a code</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="3"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="value" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The communication value such as phone number or email address</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="128"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="phoneNumber" type="PhoneNumber" minOccurs="0"/>
			<xs:element name="postAddress" type="PostAddress"/>
			<xs:element name="prefMethod" type="PrefMethod" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="id" type="xs:string"/>
		<xs:attribute name="name" type="xs:string"/>
		<xs:attribute name="uri" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="CurrencyPricedAmount">
		<xs:sequence>
			<xs:element name="amountIfRequesterBuys" type="xs:decimal" minOccurs="0"/>
			<xs:element name="amountIfRequesterSells" type="xs:decimal" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DateRange">
		<xs:sequence>
			<xs:element name="beginDate" type="xs:date" minOccurs="0"/>
			<xs:element name="endDate" type="xs:date" minOccurs="0"/>
			<xs:element name="dateType" type="DateType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DefinedField">
		<xs:sequence>
			<xs:element name="fieldName" type="xs:string"/>
			<xs:element name="fieldValue" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DiscussionElement">
		<xs:sequence>
			<xs:element name="discussionElementId" type="xs:string">
				<xs:annotation>
					<xs:documentation>Unique identification of an element in a conversation.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tradeConversationUser" type="User">
				<xs:annotation>
					<xs:documentation>A user is involved in each step of the pricingConversation.  This identifies that user.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dateTime" type="xs:dateTime"/>
			<xs:element name="comment" type="Text" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Free form comment related to this message.  When a conversationMessage is recorded this could be empty.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="relatedDiscussionElementId" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Error">
		<xs:sequence>
			<xs:element name="errorCode" type="xs:string"/>
			<xs:element name="errorDescription" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ExchangeRate">
		<xs:sequence>
			<xs:element name="quotedCurrencyPair" type="QuotedCurrencyPair"/>
			<xs:element name="rate" type="Rate"/>
			<xs:element name="spotRate" type="Rate" minOccurs="0"/>
			<xs:element name="forwardPoints" type="Rate" minOccurs="0"/>
			<xs:element name="sideRates" type="SideRates" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Expiration">
		<xs:annotation>
			<xs:documentation>Moved from TwstiMsgStreaming and TwistTradeDoc to here 0906-12</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="expirationType" default="GOOD_TO_CANCEL">
				<xs:annotation>
					<xs:documentation>GOOD_TO_TIME requires a date</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="GOOD_TO_CANCEL"/>
						<xs:enumeration value="IMMEDIATE_OR_CANCEL"/>
						<xs:enumeration value="GOOD_TO_TIME"/>
						<xs:enumeration value="GOOD_FOR_SECONDS"/>
						<xs:enumeration value="FILL_OR_KILL"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="expirationDate" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="timeToLive" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The time this order will live in seconds.   ONly needed if type requires it.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FinancialTerms">
		<xs:choice>
			<xs:element name="fxSingleLegRequest" type="FxSingleLegRequest"/>
			<xs:element name="fxSwapRequest" type="FxSwapRequest"/>
			<xs:element name="termDepositRequest" type="TermDepositRequest"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="FXFixing">
		<xs:sequence>
			<xs:element name="quotedCurrencyPair" type="QuotedCurrencyPair"/>
			<xs:element name="primaryRateSource" type="InformationSource"/>
			<xs:element name="secondaryRateSource" type="InformationSource" minOccurs="0"/>
			<xs:element name="fixingDate" type="xs:date"/>
			<xs:element name="fixingTime" type="BusinessCenterTime"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FxSingleLegPrice">
		<xs:sequence>
			<xs:element name="rate" type="Rate"/>
			<xs:element name="spotRate" type="Rate"/>
			<xs:element name="forwardPoints" type="Rate" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FxSingleLegQuote">
		<xs:sequence>
			<xs:element name="spotQuote" type="Quote"/>
			<xs:element name="forwardPointsQuote" type="Quote" minOccurs="0"/>
			<xs:element name="quotedCurrencyPair" type="QuotedCurrencyPair"/>
			<xs:element name="sideRatesQuote" type="SideRatesQuote" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FxSingleLegRequest">
		<xs:sequence>
			<xs:element name="dealtCurrency" type="Currency"/>
			<xs:element name="againstCurrency" type="Currency"/>
			<xs:choice minOccurs="0">
				<xs:annotation>
					<xs:documentation>A two-way quote has no receiver specified.  A one-way requires that one receiver be specified. </xs:documentation>
				</xs:annotation>
				<xs:element name="requesterBuys" type="xs:boolean"/>
				<xs:element name="requesterSells" type="xs:boolean"/>
			</xs:choice>
			<xs:element name="dealtCurrencyAmount" type="xs:decimal"/>
			<xs:choice>
				<xs:element name="tenor" type="FxTenor"/>
				<xs:element name="valueDate" type="xs:date"/>
			</xs:choice>
			<xs:element name="quotedCurrencyPair" type="QuotedCurrencyPair" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FxSwapQuote">
		<xs:sequence>
			<xs:element name="spotQuote" type="Quote"/>
			<xs:element name="nearPointsQuote" type="Quote" minOccurs="0"/>
			<xs:element name="farPointsQuote" type="Quote"/>
			<xs:element name="quotedCurrencyPair" type="QuotedCurrencyPair"/>
			<xs:element name="sideRatesQuote" type="FxSwapQuote" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FxSwapRequest">
		<xs:sequence>
			<xs:element name="dealtCurrency" type="Currency"/>
			<xs:element name="againstCurrency" type="Currency"/>
			<xs:choice minOccurs="0">
				<xs:annotation>
					<xs:documentation>A two-way quote has not receiver specified.  A one-way requires that on receiver be specified.  For a swap this refers to the near side</xs:documentation>
				</xs:annotation>
				<xs:element name="requesterBuys" type="xs:boolean"/>
				<xs:element name="requesterSells" type="xs:boolean"/>
			</xs:choice>
			<xs:element name="dealtCurrencyNearAmount" type="xs:decimal"/>
			<xs:element name="dealtCurrencyFarAmount" type="xs:decimal"/>
			<xs:choice>
				<xs:element name="nearTenor" type="FxTenor"/>
				<xs:element name="nearValueDate" type="xs:date"/>
			</xs:choice>
			<xs:choice>
				<xs:element name="farTenor" type="FxTenor"/>
				<xs:element name="farValueDate" type="xs:date"/>
			</xs:choice>
			<xs:element name="quotedCurrencyPair" type="QuotedCurrencyPair" minOccurs="0"/>
			<xs:element name="swapReferenceRate" type="Rate" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="GenericAcctInfo">
		<xs:sequence>
			<xs:element name="bankAccountNumberInfo" type="BankAccountNumberInfo"/>
			<xs:element name="bankInfo" type="BankInfo"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="IndustId">
		<xs:sequence>
			<xs:element name="industIdType" type="IndustIdType"/>
			<xs:element name="industIdNum" type="IndustIdNum"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="InformationSource">
		<xs:sequence>
			<xs:element name="rateSource" type="xs:string"/>
			<xs:element name="rateSourcePage" type="xs:string" minOccurs="0"/>
			<xs:element name="rateSourcePageHeading" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Instrument">
		<xs:annotation>
			<xs:documentation>.</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="fxInstrument" type="QuotedCurrencyPair"/>
			<xs:element name="mmInstrument" type="MmInstrument"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="InstrumentQuote">
		<xs:choice>
			<xs:element name="fxSingleLegQuote" type="FxSingleLegQuote"/>
			<xs:element name="fxSwapQuote" type="FxSwapQuote"/>
			<xs:element name="termDepositQuote" type="Quote"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="IntermediaryInformation">
		<xs:sequence>
			<xs:element name="intermediarySequenceNumber" type="xs:positiveInteger"/>
			<xs:element name="routing" type="Routing"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Interval">
		<xs:annotation>
			<xs:documentation>This consists of a period and optional multiplier.  Multiplier works for W, M and Y.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="periodMultiplier" type="xs:integer" minOccurs="0"/>
			<xs:element name="period" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="IntervalQuote">
		<xs:sequence>
			<xs:element name="quotedTenor" type="FxTenor"/>
			<xs:choice>
				<xs:element name="quote" type="Quote"/>
				<xs:element name="impliedVolatility" type="xs:decimal"/>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="LinkId">
		<xs:sequence>
			<xs:element name="tradeId" type="TradeId"/>
			<xs:element name="linkIdType" type="LinkIdType"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MarketQuote">
		<xs:sequence>
			<xs:element name="contributorReference" type="PartyReference"/>
			<xs:element name="quoteTime" type="QuoteTime"/>
			<xs:element name="quotedCurrencyPair" type="QuotedCurrencyPair"/>
			<xs:element name="intervalQuote" type="IntervalQuote" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MmInstrument">
		<xs:sequence>
			<xs:element name="currency" type="Currency"/>
			<xs:element name="dayCountFraction" type="DayCountFraction" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Money">
		<xs:sequence>
			<xs:element name="amount" type="xs:decimal"/>
			<xs:element name="currency" type="Currency"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OrderLimit">
		<xs:sequence>
			<xs:element name="spotRate" type="Rate"/>
			<xs:element name="points" type="Rate" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OrderParameters">
		<xs:sequence>
			<xs:element name="orderType" type="OrderType"/>
			<xs:element name="expirationDate" type="xs:dateTime"/>
			<xs:element name="allowPartialFill" type="xs:boolean" minOccurs="0"/>
			<xs:choice minOccurs="0">
				<xs:element name="limit" type="OrderLimit"/>
				<xs:element name="benchmark" type="Benchmark"/>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OrderStatus">
		<xs:annotation>
			<xs:documentation>Moved here from TwistMsgStreaming and TwistMsgPricing per 0906-13</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="partyOrderId" type="PartyOrderId"/>
			<xs:element name="remainingDealtCurrerncyAmount" type="xs:decimal"/>
			<xs:element name="limitPrice" type="FxSingleLegPrice"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OrgId">
		<xs:sequence>
			<xs:element name="orgIdType" type="OrgIdType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>changed from min 1 to min 0 0606-133</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orgIdNum">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="35"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Party">
		<xs:annotation>
			<xs:documentation>Party and all related types and elements are different from the FpML party</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="typedPartyId" type="TypedPartyId" maxOccurs="unbounded"/>
			<xs:element name="partyName" type="xs:string" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="id" type="xs:ID" use="required"/>
	</xs:complexType>
	<xs:complexType name="PartyContactInformation">
		<xs:annotation>
			<xs:documentation>changed name from PartyContactInfo to PartyContactInformation 0606-132</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="orgId" type="OrgId" minOccurs="0"/>
			<xs:element name="industId" type="IndustId" minOccurs="0"/>
			<xs:element name="name">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="70"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="legalName">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="70"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="contactInfo" type="ContactInfo"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PartyContactInfo">
		<xs:annotation>
			<xs:documentation>created 0606-134</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="PartyContactInformation">
				<xs:sequence>
					<xs:element name="individualContact" minOccurs="0" maxOccurs="2">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="name">
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:maxLength value="70"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:element>
								<xs:element name="contactInfo" type="ContactInfo"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
