<?xml version="1.0" encoding="utf-8"?><!-- edited with XMLSpy v2006 rel. 3 sp1 (http://www.altova.com) by Don Cuthbertson (private) --><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.twiststandards.org/3.0/WholesaleTrade" targetNamespace="http://www.twiststandards.org/3.0/WholesaleTrade" elementFormDefault="qualified" attributeFormDefault="unqualified">	<!-- Copyright 2006 TWIST Process Innovations, LTD. -->	<!--Working Draft-->	<!-- This schema is available for comment and the structure is subject to change -->	<!-- Version 3.1 September 2006-->	<!-- Specification Document: -->	<xs:include schemaLocation="TWIST3.1.TwistCore.TwistMsg.200609.xsd"/>	<xs:complexType name="RequestCredit">		<xs:annotation>			<xs:documentation xml:lang="en">Message:   This request is to be used to check for available credit from a list of possible providers.</xs:documentation>		</xs:annotation>		<xs:complexContent>			<xs:extension base="RequestMessage">				<xs:sequence>					<xs:element name="party" type="Party" minOccurs="2" maxOccurs="unbounded">						<xs:annotation>							<xs:documentation xml:lang="en">The parties obligated to make payments from time to time during the term of the trade. This will include, at a minimum, the principal parties involved in the swap or forward rate agreement. Other parties paying or receiving fees, commissions etc. must also be specified if referenced in other party payments.</xs:documentation>						</xs:annotation>					</xs:element>					<xs:sequence>						<xs:element name="tradeRequest" type="TradeRequest"/>						<xs:element name="creditReferenceRate" type="InstrumentQuote" minOccurs="0"/>						<xs:element name="counterPartyReference" type="PartyReference" maxOccurs="unbounded"/>						<xs:element name="availableLimitOnly" type="xs:boolean" minOccurs="0"/>					</xs:sequence>				</xs:sequence>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="RespondWithCredit">		<xs:annotation>			<xs:documentation xml:lang="en">Message:  The limit system will respond to a credit request with this message.  Multiple counterparties are includes in the creditResponses.</xs:documentation>		</xs:annotation>		<xs:complexContent>			<xs:extension base="ResponseMessage">				<xs:sequence>					<xs:element name="party" type="Party" minOccurs="2" maxOccurs="unbounded">						<xs:annotation>							<xs:documentation xml:lang="en">The parties obligated to make payments from time to time during the term of the trade. This will include, at a minimum, the principal parties involved in the swap or forward rate agreement. Other parties paying or receiving fees, commissions etc. must also be specified if referenced in other party payments.</xs:documentation>						</xs:annotation>					</xs:element>					<xs:sequence>						<xs:element name="creditResponses">							<xs:complexType>								<xs:sequence>									<xs:element name="creditResponse" type="CreditResponse" maxOccurs="unbounded"/>								</xs:sequence>							</xs:complexType>						</xs:element>					</xs:sequence>				</xs:sequence>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="DrawDownCredit">		<xs:annotation>			<xs:documentation xml:lang="en">Message: Subsequent to executing a trade for which a credit limit request was made, this message actually draws down the limit that was reserved by the earlier request.  The original request is submitted since it may be needed by the limit system to reset the available amount before drawing down the actual amount. </xs:documentation>		</xs:annotation>		<xs:complexContent>			<xs:extension base="RequestMessage">				<xs:sequence>					<xs:element name="party" type="Party" minOccurs="2" maxOccurs="unbounded">						<xs:annotation>							<xs:documentation xml:lang="en">The parties obligated to make payments from time to time during the term of the trade. This will include, at a minimum, the principal parties involved in the swap or forward rate agreement. Other parties paying or receiving fees, commissions etc. must also be specified if referenced in other party payments.</xs:documentation>						</xs:annotation>					</xs:element>					<xs:element name="drawDown" type="DrawDown"/>				</xs:sequence>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="ReplenishCredit">		<xs:annotation>			<xs:documentation xml:lang="en">Message: After executing a trade limit requests against providers that did not win the detail must be replenished.  This message is used for this purpose.</xs:documentation>		</xs:annotation>		<xs:complexContent>			<xs:extension base="RequestMessage">				<xs:sequence>					<xs:element name="party" type="Party" minOccurs="2" maxOccurs="unbounded">						<xs:annotation>							<xs:documentation xml:lang="en">The parties obligated to make payments from time to time during the term of the trade. This will include, at a minimum, the principal parties involved in the swap or forward rate agreement. Other parties paying or receiving fees, commissions etc. must also be specified if referenced in other party payments.</xs:documentation>						</xs:annotation>					</xs:element>					<xs:element name="replenish" type="Replenish"/>				</xs:sequence>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="RequestCreditCarveOut">		<xs:annotation>			<xs:documentation xml:lang="en">Message:   This is used to carve out credit for a particular usage</xs:documentation>		</xs:annotation>		<xs:complexContent>			<xs:extension base="RequestMessage">				<xs:sequence>					<xs:element name="party" type="Party" minOccurs="2" maxOccurs="unbounded">						<xs:annotation>							<xs:documentation xml:lang="en">The parties obligated to make payments from time to time during the term of the trade. This will include, at a minimum, the principal parties involved in the swap or forward rate agreement. Other parties paying or receiving fees, commissions etc. must also be specified if referenced in other party payments.</xs:documentation>						</xs:annotation>					</xs:element>					<xs:element name="partyReference" type="PartyReference" maxOccurs="unbounded">						<xs:annotation>							<xs:documentation>For each party a credit line is requested</xs:documentation>						</xs:annotation>					</xs:element>					<xs:element name="reserveExpireTime" type="xs:dateTime">						<xs:annotation>							<xs:documentation>This is the lenght of time requested for the credit carve out.</xs:documentation>						</xs:annotation>					</xs:element>				</xs:sequence>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="RespondCreditCarveOut">		<xs:annotation>			<xs:documentation xml:lang="en">Message:   This is used to carve out credit for a particular usage</xs:documentation>		</xs:annotation>		<xs:complexContent>			<xs:extension base="ResponseMessage">				<xs:sequence>					<xs:element name="party" type="Party" maxOccurs="unbounded">						<xs:annotation>							<xs:documentation xml:lang="en">The parties obligated to make payments from time to time during the term of the trade. This will include, at a minimum, the principal parties involved in the swap or forward rate agreement. Other parties paying or receiving fees, commissions etc. must also be specified if referenced in other party payments.</xs:documentation>						</xs:annotation>					</xs:element>					<xs:element name="partyCreditCarveOut" type="PartyCreditCarveOut" maxOccurs="unbounded"/>				</xs:sequence>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="CreditResponse">		<xs:annotation>			<xs:documentation>Credit response from a single counterparty</xs:documentation>		</xs:annotation>		<xs:sequence>			<xs:element name="partyTradeIdentifier" type="PartyTradeIdentifier"/>			<xs:element name="reserveId" type="ReserveId"/>			<xs:element name="limitType" minOccurs="0">				<xs:annotation>					<xs:documentation>The limit system will provide the type of limit being reported.</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="limitCurrency" type="Currency" minOccurs="0"/>			<xs:element name="availableCredit" type="AvailableCredit"/>			<xs:element name="alternateAvailableCredit" type="AvailableCredit" minOccurs="0">				<xs:annotation>					<xs:documentation>In the event that no credit is available from the requrested counterparty an alternate can be suggested.</xs:documentation>				</xs:annotation>			</xs:element>		</xs:sequence>	</xs:complexType>	<xs:complexType name="AvailableCredit">		<xs:annotation>			<xs:documentation>Type that describes the available creidt.</xs:documentation>		</xs:annotation>		<xs:sequence>			<xs:element name="counterPartyReference" type="PartyReference"/>			<xs:element name="remainingAvailableAmount" type="Money"/>			<xs:element name="originalAvailableAmount" type="Money" minOccurs="0"/>			<xs:element name="lineAmount" type="Money" minOccurs="0"/>		</xs:sequence>	</xs:complexType>	<xs:complexType name="DrawDown">		<xs:sequence>			<xs:element name="replenish" type="Replenish"/>			<xs:element name="instrumentQuote" type="InstrumentQuote"/>		</xs:sequence>	</xs:complexType>	<xs:complexType name="PartyCreditCarveOut">		<xs:sequence>			<xs:element name="creditLine" type="CreditLine" maxOccurs="unbounded">				<xs:annotation>					<xs:documentation>There may be lines for diferent instruments.</xs:documentation>				</xs:annotation>			</xs:element>		</xs:sequence>	</xs:complexType>	<xs:complexType name="CreditLine">		<xs:sequence>			<xs:element name="creditAmount" type="Money"/>			<xs:element name="instrumentType" type="xs:string" minOccurs="0">				<xs:annotation>					<xs:documentation>This identifies the instrucment for which the credit applies.</xs:documentation>				</xs:annotation>			</xs:element>		</xs:sequence>	</xs:complexType>	<xs:complexType name="Replenish">		<xs:sequence>			<xs:element name="partyTradeIdentifier" type="PartyTradeIdentifier"/>			<xs:element name="tradeRequest" type="TradeRequest"/>			<xs:element name="reserveId" type="ReserveId"/>			<xs:element name="counterPartyReference" type="PartyReference"/>		</xs:sequence>	</xs:complexType></xs:schema>